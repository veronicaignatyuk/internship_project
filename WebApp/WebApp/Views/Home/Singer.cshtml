@model WebApp.Models.Singer

<h1>@Model.Name</h1>
<div class="singer_info">
    <img src="@Url.Content(@Model.BigPicture)" />
    <div class="left">
        <p>@Model.CountSongs Suite Chords</p>
        @Model.Biography 
    </div>
</div>
<div id="container">
    <table class="table table-hover table-condensed">
        <caption>
            <strong> Suite Chords</strong>
        </caption>
        <thead>
            <tr>
                <td class='artist_name' colspan="2"><p>Title</p></td>
                <td><p>Video</p></td>
                <td><p>Count views</p></td>
            </tr>
        </thead>
        <tr>
            <td class='artist_name' colspan="2"><p>Title</p></td>
            <td><p>Video</p></td>
            <td><p>Count views</p></td>
        </tr>
    </table>

</div>

    <table class="table table-hover table-condensed">
        <caption>
            <strong>  Популярные исполнители</strong>
        </caption>
       <thead>
             <tr>
                <td class='artist_name' colspan="2"><p>Singers</p></td>
                <td ><p>Count songs</p></td>
                <td><p>Count views</p></td>
            </tr>
        </thead>
        @foreach (var s in ViewBag.Singers)
        {
            <tr>
                <td><img src="@Url.Content(s.Picture)" /></td>
                <td class='artist_name'>
                @Html.ActionLink((string)@s.Name, "Singer", "Singer")</td>
                <td>@s.CountSongs</td>
                <td>@s.CountViews</td>
            </tr>
        }
    </table>
